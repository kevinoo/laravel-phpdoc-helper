{
    "name": "kevinoo/laravel-phpdoc-helper",
    "version": "1.0.0",
    "type": "library",
    "description": "Help IDE to know the PHPDoc for classes use via Facade",
    "keywords": ["PHPDoc","framework","laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Lucich",
            "email": "info@lucichkevin.it",
            "homepage": "https://www.lucichkevin.it",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@lucichkevin.it"
    },
    "funding": [
        {
            "type": "paypal",
            "url": "https://www.paypal.com/paypalme/kevinlucich"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "kevinoo\\LaravelPhpDocHelper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "tests": "phpunit --colors=always --verbose"
    },
    "extra": {
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
